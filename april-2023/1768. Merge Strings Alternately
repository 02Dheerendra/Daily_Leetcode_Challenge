class Solution:
    def mergeAlternately(self, word1: str, word2: str) -> str:
        ans=""
        i,j=0,0
        while(i<=word1.length() and j<=word2.length()):
            if(i<word1.length()):
                ans+=word1[i]
            if(j<word2.length()):
                ans+=word2[j]
            if(i==word1.length() and j==word2.length()):
                break;
        return ans;
